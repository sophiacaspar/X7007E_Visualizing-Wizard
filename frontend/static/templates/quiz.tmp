{{define "Title"}}
	{{ .Title}}
{{end}}

{{define "Body"}}
<html>


<script src="/static/js/getIP.js"></script> 
<script src="/static/js/result.js"></script> 
<script src="/static/js/quiz.js"></script>
<script src="/static/js/questions.js"></script> 

<!-- Table scripts and css -->
<script src="/static/js/tables/tablesaw.jquery.js"></script>
<script src="/static/js/tables/tablesaw-init.js"></script>
<link rel="stylesheet" href="/static/css/tables/click.css">
<link rel="stylesheet" href="/static/css/tables/tablesaw.css">


<script src="/static/js/tables/scroll.js"></script> 
<link href="/static/css/tables/scroll.css" rel="stylesheet">

<script src="/static/js/tables/squish.js"></script> 
<link href="/static/css/tables/squish.css" rel="stylesheet">

<link rel="stylesheet" href="/static/css/tables/rowCollapse.css">





<body onload="firstQuestion()">

<div id="quiz">
<div class="row">
	<div class="question" >
		<h1 id="question"></h1>
		<h3>Välj det alternativ som passar bäst in på din tabell</h3>
			
	</div>
</div>



	<div id="answers" class="btn-container">
		<div class="row">
	    <div class="col-md-3">
	        <a class="btn btn-success btn-lg btn-block btn-huge" id="ans1" onclick="getAnswers('ans1')"></a>
	    </div>
	    <div class="col-md-3">
	        <a class="btn btn-success btn-lg btn-block btn-huge" id="ans2" onclick="getAnswers('ans2')"></a>
	    </div>
	    <div class="col-md-3">
	        <a class="btn btn-success btn-lg btn-block btn-huge" id="ans3" onclick="getAnswers('ans3')"></a>
	    </div>
	    <div class="col-md-3">
	        <a class="btn btn-success btn-lg btn-block btn-huge" id="ans4" onclick="getAnswers('ans4')"></a>
	    </div>
	</div>

	<div class="row dontKnow">

	        <a class="btn btn-danger btn-lg btn-block btn-huge" id="ans5" onclick="getAnswers('Null')">Övrigt/Vet inte</a>

	</div>
</div>
</div>

<!-- ------------------------------- RESULT ---------------------- -->
<div id="result" display="none">
	<div id="resultText"></div>
	<div id="tableHelp">
		<textarea id="tableInput" rows="4" cols="70" placeholder="Skriv in din tabell här. Separera kolumner med komman (,) och rader med radbrytning."></textarea>
		<br><button type="button" class="btn btn-info" onclick="getTable()">Generera tabell</button>
		<br>
		
		<div id="exampleTable"></div>
	</div>

</div>

</div>

</body>
</html>

{{end}}